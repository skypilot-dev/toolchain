{
  "name": "@skypilot/toolchain",
  "version": "0.3.1",
  "description": "Toolchain for Node projects",
  "author": "William Thorsen",
  "license": "UNLICENSED",
  "homepage": "https://github.com/skypilot/toolchain#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skypilot/toolchain.git"
  },
  "bugs": {
    "url": "https://github.com/skypilot/toolchain/issues"
  },
  "bin": {
    "toolchain": "./lib/scripts/init.js"
  },
  "files": [
    "/lib",
    "babel.config.js",
    "eslint.config.js",
    "jest.config.js",
    "jest.integration.config.js",
    "jest.standalone.config.js",
    "husky.config.js",
    "lint-staged.config.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "jest",
    "typescript"
  ],
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && yarn run copy-templates && babel ./src --out-dir ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "copy-templates": "mkdir lib && cp -a src/templates lib/",
    "prepublishOnly": "yarn run check-types && yarn run test && yarn run build && yarn run generate-typings",
    "test": "jest --config jest.standalone.config.js"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-jest": "^24.9.0",
    "deepmerge": "^4.0.0",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/tmp": "^0.1.0",
    "tmp": "^0.1.0"
  },
  "peerDependencies": {}
}
