{
  "name": "@skypilot/toolchain",
  "version": "0.2.9",
  "description": "Toolchain for Node projects",
  "author": "William Thorsen",
  "license": "UNLICENSED",
  "homepage": "https://github.com/skypilot/toolchain#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skypilot/toolchain.git"
  },
  "bugs": {
    "url": "https://github.com/skypilot/toolchain/issues"
  },
  "bin": {
    "toolchain": "./lib/scripts/init.js"
  },
  "files": [
    "/lib",
    "babel.config.js",
    "eslint.config.js",
    "jest.config.js",
    "jest.config.combo.js",
    "jest.config.integration.js",
    "jest.config.unit.js",
    "husky.config.js",
    "lint-staged.config.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "jest",
    "typescript"
  ],
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && npm run copy-templates && babel ./src --out-dir ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "copy-templates": "mkdir lib && cp -a src/templates lib/",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "deepmerge": "^4.0.0",
    "eslint": "^6.2.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/tmp": "^0.1.0",
    "tmp": "^0.1.0"
  },
  "peerDependencies": {}
}
